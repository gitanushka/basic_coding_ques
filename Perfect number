#include <iostream>
using namespace std;

int main ()
{
    int num, sum = 0;
    cin>>num;
    int i = 1;   
    while(i <= num/2)  
    {
        if(num % i == 0)  
            sum = sum + i;  
        i++;  
    }  
    
    if(sum == num)
        cout << num << " is a perfect number";
    else
        cout << num << " is not a perfect number";
    
    return 0;
}
