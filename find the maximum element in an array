#include <bits/stdc++.h>
using namespace std;

 void maxElement(int arr[], int n){
     
     int max = 0;
     for(int i=0;i<n;i++){
         if(max<arr[i]){
             max = arr[i];
         }
     }
     cout<<max;
}


int main() {
    // Write C++ code here
    int n;
    cin>>n;
    int arr[n];
    
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    maxElement(arr, n);
    return 0;
}
