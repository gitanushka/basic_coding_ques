#include <bits/stdc++.h>
using namespace std;

int checkpalindrome(string str){
    int i = 0, j = str.length()-1;
    while(i<j){
        if(str[i]==str[j]){
            i++;
            j--;
        }
        else{
            return 0;
        }
    }
    return 1;
    
}

int main() {
    // Write C++ code here
    string s;
    cin>>s;
    cout << checkpalindrome(s);
    return 0;
}
