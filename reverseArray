#include <bits/stdc++.h>
using namespace std;

 void reverseArray(int arr[], int n){
     
     int i=0, j=n-1;
     while(i<j){
         swap(arr[i],arr[j]);
         i++;
         j--;
     }
     
     for(int k=0;k<n;k++){
         cout<<arr[k]<<" ";
     }
     
}


int main() {
    // Write C++ code here
    int n;
    cin>>n;
    int arr[n];
    
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    reverseArray(arr, n);
    return 0;
}
