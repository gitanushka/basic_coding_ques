#include <bits/stdc++.h>
using namespace std;

int checkNumericalDigits(string str){
    int count = 0;
    for(int i=0;i<str.length();i++){
        if(str[i]>='0' && str[i]<='9'){
            count++;
        }
    }
    return count;
    
}

int main() {
    // Write C++ code here
    string s;
    cin>>s;
    cout << checkNumericalDigits(s);
    return 0;
}
