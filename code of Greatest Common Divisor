// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;

int GCD(int n1, int n2){
    int gcd = 1;
    if(n1==0 || n2==0){
        return 0;
    }
    if(n1<n2){
        for(int i=1;i<=n1;i++){
            if(n1%i==0 && n2%i==0){
                gcd = i;
            }
        }
    }    
        else{
            for(int i=1;i<=n2;i++){
                if(n1%i==0 && n2%i==0){
                    gcd = i;
                }
            }
        }
    
    return gcd;
}

int main() {
    // Write C++ code here
    int num1,num2;
    cin>>num1>>num2;
    cout << GCD(num1,num2);

    return 0;
}
